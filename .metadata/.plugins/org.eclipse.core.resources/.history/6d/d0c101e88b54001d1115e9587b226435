package hello;

/**
* @author Diego
*/

public class hello {
	
		public static void main(String args[]) {
		System.out.println("Hello World");
		
			int valor = 1;
			
			switch (valor) {
			case 0:
				System.out.println("Valor é 0");
				break;
			case 1:
				System.out.println("Valor é 1");
				break;
			default:
				System.out.println("Valor é 2");
				break;
			}
		
		}
	
} 